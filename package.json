{
  "name": "intravision_tasks",
  "type": "module",
  "license": "MIT",
  "version": "0.0.1",
  "description": "intravision_tasks",
  "scripts": {
    "watch": "yarn devTools && vite --open",
    "watch:po": "po-extract --outExt=.ts --outDir=./src/i18n/dictionaries --watch --fixPo",
    "watch:storybook": "storybook dev -p 6006",
    "build": "vite build",
    "build:po": "po-extract --outExt=.ts --outDir=./src/i18n/dictionaries --fixPo",
    "build:storybook": "storybook build",
    "prettier": "prettier --check ./",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "postinstall": "husky install",
    "typecheck": "tsc",
    "devTools": "node devTools/index.js"
  },
  "dependencies": {
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test-runner": "^0.21.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.23.3",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-storybook": "^0.11.2",
    "formik": "^2.4.6",
    "history": "^5.3.0",
    "husky": "^9.1.7",
    "node-fetch": "^3.3.2",
    "po-extract": "^2.1.9",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intl": "^7.1.4",
    "react-redux": "9.2.0",
    "react-router-dom": "^7.1.3",
    "redux": "4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.3.0",
    "storybook": "^8.5.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "typescript-fsa-factory": "^0.0.12",
    "typescript-fsa-reducers": "^1.2.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.0.11",
    "vite-plugin-svgr": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": "20.*",
    "yarn": "1.22.*"
  }
}
